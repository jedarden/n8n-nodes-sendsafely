{
  "name": "n8n-nodes-sendsafely",
  "version": "1.0.0",
  "description": "n8n node for SendSafely secure file transfer and encryption",
  "license": "MIT",
  "homepage": "https://github.com/jedarden/n8n-nodes-sendsafely",
  "author": {
    "name": "Jed Arden",
    "email": "jed@ardenone.com",
    "url": "https://github.com/jedarden"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedarden/n8n-nodes-sendsafely.git"
  },
  "bugs": {
    "url": "https://github.com/jedarden/n8n-nodes-sendsafely/issues"
  },
  "main": "dist/index.js",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "sendsafely",
    "secure-transfer",
    "encryption",
    "file-transfer"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SendSafelyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SendSafely/SendSafely.node.js"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "dependencies": {
    "@sendsafely/sendsafely": "^2.0.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
